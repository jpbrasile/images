import React, { useState, useEffect } from "https://esm.sh/react@18.2.0";
import { createRoot } from "https://esm.sh/react-dom@18.2.0/client";
import { email } from "https://esm.town/v/std/email";

interface FormData {
  name: string;
  email: string;
  organization: string;
  message: string;
}

function App() {
  // State management for form submission status and dark mode toggle
  const [formStatus, setFormStatus] = useState<string | null>(null);
  const [darkMode, setDarkMode] = useState(false);

  // Apply dark mode class to document when state changes
  useEffect(() => {
    if (darkMode) {
      document.documentElement.classList.add('dark');
    } else {
      document.documentElement.classList.remove('dark');
    }
  }, [darkMode]);

  // Form submission handler with mock API endpoint
  const handleSubmit = async (e: React.FormEvent<HTMLFormElement>) => {
    e.preventDefault();
    const form = e.currentTarget;
    const formData = new FormData(form);
    const data: FormData = {
      name: formData.get('name') as string,
      email: formData.get('email') as string,
      organization: formData.get('organization') as string,
      message: formData.get('message') as string,
    };

    try {
      const response = await fetch('/submit', {
        method: 'POST',
        headers: { 'Content-Type': 'application/json' },
        body: JSON.stringify(data),
      });

      if (response.ok) {
        setFormStatus("Formulaire envoy√© avec succ√®s!");
        form.reset();
      } else {
        setFormStatus("Erreur lors de l'envoi du formulaire. Veuillez r√©essayer.");
      }
    } catch (error) {
      setFormStatus("Erreur lors de l'envoi du formulaire. Veuillez r√©essayer.");
    }
  };

  return (
    <div className={`flex flex-col min-h-screen ${darkMode ? 'dark' : ''}`}>
      {/* Navigation */}
      <nav className="bg-green-800 dark:bg-gray-900 text-white p-4">
